<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.studio.tranquil.notee" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >


        <activity
            android:name="com.studio.tranquil.notee.splas"
        >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.studio.tranquil.notee.Main_Screen"
            android:label="@string/app_name"
            android:icon="@mipmap/ic_launcher"
            >

        </activity>
        <activity
            android:name="com.studio.tranquil.notee.Add_Update_User"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"


        />



        <activity
            android:name="com.studio.tranquil.notee.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
    </application>

</manifest>
